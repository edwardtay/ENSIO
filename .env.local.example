# Required — Anthropic API key for the AI chat agent
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Required — WalletConnect project ID (https://cloud.walletconnect.com)
NEXT_PUBLIC_WC_PROJECT_ID=your-walletconnect-project-id

# Optional — Ethereum RPC URL for ENS resolution (defaults to https://eth.llamarpc.com)
ETH_RPC_URL=https://eth.llamarpc.com

# Optional — Circle API key for programmable wallets and CCTP bridging
CIRCLE_API_KEY=

# Optional — Stork API key for real-time price oracle data
STORK_API_KEY=

# Optional — CCIP-Read gateway signer (private key for signing offchain ENS responses)
GATEWAY_SIGNER_KEY=
# Optional — Public address of the gateway signer (must match GATEWAY_SIGNER_KEY)
NEXT_PUBLIC_GATEWAY_SIGNER_ADDRESS=
